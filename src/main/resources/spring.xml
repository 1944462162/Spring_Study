<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.imust.service.Impl.Account" id="account">
        <constructor-arg name="name" value="张三"/>
        <constructor-arg name="age" value="18"/>
        <constructor-arg name="brithday" ref="day"/>
    </bean>
    <bean class="com.imust.service.Impl.Account" factory-bean="account" factory-method="Account"/>


    <bean id="day" class="java.util.Date"/>
</beans>