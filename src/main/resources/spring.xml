    <bean class="com.imust.service.Impl.Account" id="account">
        <property name="name" value="张三"/>
        <property name="age" value="18"/>
        <property name="brithday" ref="day"/>

        <property name="myList">
            <list>
                <value>AAA</value>
                <value>BBB</value>
                <value>CCC</value>
            </list>
        </property>

        <property name="myMap">
            <map>
                <entry key="A" value="1"/>
                <entry key="B" value="2"/>
            </map>
        </property>
    </bean>
    <!--<bean class="com.imust.service.Impl.Account" factory-bean="account" factory-method="Account"/>-->


    <bean id="day" class="java.util.Date"/>
</beans>